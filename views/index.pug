doctype html
html(lang="en")
	head
		meta(charset="UTF-8")
		title Logowanie przez Google
	body
		h1 Zaloguj się przez Google
		form(action="http://localhost:3000/auth/google", method="GET")
			p 
				| Login: 
				input(type="text", name="login")
			p 
				| Hasło: 
				input(type="text", name="haslo")
			input(type="submit", value="Zaloguj")
